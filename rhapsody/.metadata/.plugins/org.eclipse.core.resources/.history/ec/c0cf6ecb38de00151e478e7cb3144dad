	  ${uniqueTrigger.Name} : in none,	
	[#else]
		[#if (uniqueTrigger.ArgSize > 1)]
	  ${uniqueTrigger.Name} : in Double_Floor,
		[#else]
	  ${uniqueTrigger.Name} : in int,
		[/#if]
	  
	[/#if]
[/#if]
[/#foreach]
	${process.outPorts.Name}
    ]
[/#template]
