[#package generateProcess]

[#template public genProcessFIFO(process : fiacreMM.Process)]
	[#foreach var in process.variables]
		[#if var.datatype.Name == "queue"]
		[#include genFIFOelemtype(var.datatype, process)]
		type ${var.Name} is queue ${var.datatype.Size} of ${var.datatype.elementType}
		[/#if]
	[/#foreach]
[/#template]
