[#package generateUtile]

[#template public handleEnd(set,SIZE,symb)]
[#set nbElement = 0]
[#foreach element in set]
	[#if SIZE==0]
		${element.Name}
	[#else]
		[#if nbElement == SIZE-1]
			${element.Name}
		[#else]
			${element.Name}${symb}
			[#set nbElement = nbElement+1]
		[/#if]
	[/#if]
[/#foreach]

[/#template]
